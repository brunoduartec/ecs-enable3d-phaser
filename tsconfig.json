{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowUnreachableCode": false,
    "sourceMap": true,
    "strictPropertyInitialization": false,
    "lib": ["dom", "es6", "esnext"]
  }
}
